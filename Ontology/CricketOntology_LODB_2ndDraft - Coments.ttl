###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/belongsToTeam
:belongsToTeam rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasPlayer ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Player ;
               rdfs:range :Team ;
               rdfs:comment "It represents an object property that links player with the team."@en ;
               rdfs:label "belongsToTeam"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasBattingTeam
:hasBattingTeam rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain :Innings ;
                rdfs:range :Team ;
                rdfs:comment "Its an object property for innings. It represents that it has a batting team."@en ;
                rdfs:label "hasBattingTeam"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasDelivery
:hasDelivery rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Innings ;
             rdfs:range :Delivery ;
             rdfs:comment "It represents balls played within the innings. Our data contains ball by ball data as delivery."@en ;
             rdfs:label "Delivery" .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasFirstInning
:hasFirstInning rdf:type owl:ObjectProperty ;
                owl:inverseOf :isFirstInningOfMatch ;
                rdfs:domain :Match ;
                rdfs:range :Inning1 ;
                rdfs:comment "It links first innings with the match." ;
                rdfs:label "hasFirstInning"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasOutcomeToss
:hasOutcomeToss rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Match ;
                rdfs:range :Toss .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasPlayer
:hasPlayer rdf:type owl:ObjectProperty ;
           rdfs:domain :Team ;
           rdfs:range :Player .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasSecondInning
:hasSecondInning rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isSecondInningOfMatch ;
                 rdfs:domain :Match ;
                 rdfs:range :Inning2 ;
                 rdfs:comment "It links second innings with the match."@en ;
                 rdfs:label "hasSecondInnings"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasWinner
:hasWinner rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Toss ;
           rdfs:range :Team ;
           rdfs:comment "Its links toss decision with the team. Data contains toss winner teams of every innings."@en ;
           rdfs:label "hasWinner"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/isFirstInningOfMatch
:isFirstInningOfMatch rdf:type owl:ObjectProperty ;
                      rdfs:domain :Inning1 ;
                      rdfs:range :Match ;
                      rdfs:comment "Its a property of first innings which represents that it belongs to a match."@en ;
                      rdfs:label "isFirstInningsOfMatch"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/isOfficiatedBy
:isOfficiatedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf :officiatedInMatch ;
                rdfs:domain :Match ;
                rdfs:range :Umpire ;
                rdfs:comment "It links Umpire with the Match."@en ;
                rdfs:label "isOfficiatedBy"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/isParticipatedBy
:isParticipatedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain :Match ;
                  rdfs:range :Team ;
                  rdfs:comment "It links team with the match. it represents that teams participate in the match."@en ;
                  rdfs:label "isParticipatedBy"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/isSecondInningOfMatch
:isSecondInningOfMatch rdf:type owl:ObjectProperty ;
                       rdfs:domain :Inning2 ;
                       rdfs:range :Match ;
                       rdfs:comment "Its a property of second innings which represents that it belongs to a match."@en ;
                       rdfs:label "isSecondInningsOfMatch"@en .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/officiatedInMatch
:officiatedInMatch rdf:type owl:ObjectProperty ;
                   rdfs:domain :Umpire ;
                   rdfs:range :Match .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasDates
:hasDates rdf:type owl:DatatypeProperty ;
          rdfs:domain :Match ;
          rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasDecision
:hasDecision rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Toss ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "bat" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "field" ;
                                                 rdf:rest rdf:nil
                                               ]
                                    ]
                        ] .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasManofMatch
:hasManofMatch rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Match ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasTotalBalls
:hasTotalBalls rdf:type owl:DatatypeProperty ;
               rdfs:domain :Delivery ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:onDatatype xsd:integer ;
                            owl:withRestrictions ( [ xsd:minInclusive 120
                                                   ]
                                                   [ xsd:maxInclusive 125
                                                   ]
                                                 )
                          ] .


###  http://www.semanticweb.org/mehul/ontologies/2024/10/untitled-ontology-25/hasVenue
:hasVenue rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Match ;
          rdfs:range xsd:string .


